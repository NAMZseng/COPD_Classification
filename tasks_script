 nohup python -u ./utils/data_process.py > ./utils/out.log &

----------------------------------train----------------------------------
   nohup python -u train.py \
 --net_name tinynet \
 --data_root_path /data/zengnanrong/lung_seg_normal_resize \
 --use_gpu True \
 --batch_size 8 \
 --num_epochs 200 \
 --learning_rate 0.001 \
 --save_model_name tinynet_img_lrelu_224_aug3.pth \
 --cuda_device 1 \
 > ./log/tinynet/tinynet_img_lrelu_224_aug3_train.log &

 ----------------------------------test----------------------------------
    nohup python -u test.py \
 --net_name resnet_3D \
 --data_root_path /data/zengnanrong/lung_seg_normal_resize \
 --use_gpu True \
 --batch_size 8 \
 --save_model_name resnet10_img_finetune_adam_lrelu_224_aug.pth \
 --result_file resnet10_img_finetune_adam_lrelu_224_aug.xlsx \
 --cuda_device 1 \
 > ./log/3D_resnet/resnet10_img_finetune_adam_lrelu_224_aug_test.log &



